{
    "extends": "airbnb-base",
    "rules": {
        "function-paren-newline": "off",
        "indent": ["error", 4],
        "no-param-reassign": "off",
        "object-curly-newline": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
    },
    "plugins": [
        "jest"
    ],
    "env": {
        "jest/globals": true,
        "browser": true
    },
    "globals": {
        "bonsai": true,
        "stage": true,
        "Path": true
    }
}